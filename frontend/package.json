{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "bunx --bun vite --port 4000",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prettier:check": "bunx --bun prettier --check --log-level=warn \"src/**/*.{ts,tsx,css}\"",
    "prettier:format": "bunx --bun prettier --write --log-level=warn \"src/**/*.{ts,tsx,css}\"",
    "generate:api": "bun run tools/generateApi.ts"
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "antd": "^5.12.8",
    "classnames": "^2.5.1",
    "effector": "^23.1.0",
    "effector-forms": "^1.3.4",
    "effector-react": "^23.1.0",
    "ky": "^1.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.1"
  },
  "devDependencies": {
    "@types/bun": "^1.0.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/react-router-dom": "^5.3.3",
    "@types/web": "^0.0.134",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "prettier": "^3.1.1",
    "swagger-typescript-api": "^13.0.3",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}
